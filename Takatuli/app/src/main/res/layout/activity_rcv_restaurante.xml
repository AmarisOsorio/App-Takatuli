    tools:context=".activity_rcvRestaurante">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="409dp"
        android:layout_height="353dp"
        android:layout_marginTop="-275dp"
        app:layout_constraintBottom_toBottomOf="parent"
